"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Dear ${object.name},\n"
"        <br/><br/>\n"
"        Please find attached an outstanding documents statement,\n"
"        dated ${format_date(ctx['date_end'])}.\n"
"        <br/><br/>\n"
"        If you have any question, do not hesitate to contact us.\n"
"        <br/><br/>\n"
"        Regards,\n"
"        % if user.signature:\n"
"            <br/>\n"
"            ${user.signature | safe}\n"
"        % endif\n"
"    </p>\n"
"</div>\n"
"        "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Caro(a) ${object.name},\n"
"       <br/><br/>\n"
"        Enviamos em anexo um extrato de documentos pendentes no dia ${format_date(ctx['date_end'])}.\n"
"       <br/><br/>\n"
"        Caso haja alguma questão, não hesite em contactar-nos.\n"
"       <br/><br/>\n"
"        Cumprimentos,\n"
"        % if user.signature:\n"
"           <br/>\n"
"            ${user.signature | safe}\n"
"        % endif\n"
"    </p>\n"
"</div>\n"
"        "

