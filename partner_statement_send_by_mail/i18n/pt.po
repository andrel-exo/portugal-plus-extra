#. module: partner_statement_send_by_mail
#: model:mail.template,body_html:partner_statement_send_by_mail.email_template_partner_activity_statement
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Dear <t t-out=\"object.name or ''\">Deco Addict</t>,\n"
"        <br><br>\n"
"        Please find attached a balance statement,\n"
"        dated between <t t-out=\"format_date(ctx['date_start']) or ''\">Deco "
"Addict</t> and <t t-out=\"format_date(ctx['date_end']) or ''\">Deco Addict</"
"t>.\n"
"        <br><br>\n"
"        If you have any question, do not hesitate to contact us.\n"
"        <br><br>\n"
"        Regards,\n"
"        <t t-if=\"not is_html_empty(user.signature)\">\n"
"            <br><br>\n"
"            <t t-out=\"user.signature or ''\">--<br>Mitchell Admin</t>\n"
"        </t>\n"
"    </p>\n"
"</div>\n"
"        "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Caro(a) <t t-out=\"object.name or ''\">Deco Addict</t>,\n"
"        <br><br>\n"
"        Enviamos em anexo um extrato de movimentos entre\n"
"        <t t-out=\"format_date(ctx['date_start']) or ''\">???</t> e <t t-out="
"\"format_date(ctx['date_end']) or ''\">???</t>.\n"
"        <br><br>\n"
"        Caso haja alguma questão, não hesite em contactar-nos.\n"
"        <br><br>\n"
"        Cumprimentos,\n"
"        <t t-if=\"not is_html_empty(user.signature)\">\n"
"            <br><br>\n"
"            <t t-out=\"user.signature or ''\">--<br>Mitchell Admin</t>\n"
"        </t>\n"
"    </p>\n"
"</div>\n"
"        "

#. module: partner_statement_send_by_mail
#: model:mail.template,body_html:partner_statement_send_by_mail.email_template_partner_outstanding_statement
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Dear <t t-out=\"object.name or ''\">Deco Addict</t>,\n"
"        <br><br>\n"
"        Please find attached an outstanding documents statement,\n"
"        dated <t t-out=\"format_date(ctx['date_end']) or ''\">Deco Addict</"
"t>.\n"
"        <br><br>\n"
"        If you have any question, do not hesitate to contact us.\n"
"        <br><br>\n"
"        Regards,\n"
"        <t t-if=\"not is_html_empty(user.signature)\">\n"
"            <br><br>\n"
"            <t t-out=\"user.signature or ''\">--<br>Mitchell Admin</t>\n"
"        </t>\n"
"    </p>\n"
"</div>\n"
"        "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Caro(a) <t t-out=\"object.name or ''\">Deco Addict</t>,\n"
"        <br><br>\n"
"         Enviamos em anexo um extrato de documentos pendentes no dia <t t-"
"out=\"format_date(ctx['date_end']) or ''\">???</t>.\n"
"        <br><br>\n"
"        Caso haja alguma questão, não hesite em contactar-nos.\n"
"        <br><br>\n"
"        Cumprimentos,\n"
"        <t t-if=\"not is_html_empty(user.signature)\">\n"
"            <br><br>\n"
"            <t t-out=\"user.signature or ''\">--<br>Mitchell Admin</t>\n"
"        </t>\n"
"    </p>\n"
"</div>\n"
"        "

