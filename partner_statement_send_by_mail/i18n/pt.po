#. module: partner_statement_send_by_mail
#: model:mail.template,body_html:partner_statement_send_by_mail.email_template_partner_activity_statement
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Dear ${object.name},\n"
"        <br/><br/>\n"
"        Please find attached a balance statement,\n"
"        dated between ${format_date(ctx['date_start'])} and ${format_date(ctx['date_end'])}.\n"
"        <br/><br/>\n"
"        If you have any question, do not hesitate to contact us.\n"
"        <br/><br/>\n"
"        Regards,\n"
"        % if user.signature:\n"
"            <br/>\n"
"            ${user.signature | safe}\n"
"        % endif\n"
"    </p>\n"
"</div>\n"
"        "
msgstr ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"    <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"        Caro(a) ${object.name},\n"
"       <br/><br/>\n"
"        Enviamos em anexo um extrato de movimentos entre ${format_date(ctx['date_start'])} e ${format_date(ctx['date_end'])}.\n"
"       <br/><br/>\n"
"        Caso haja alguma questão, não hesite em contactar-nos.\n"
"       <br/><br/>\n"
"        Cumprimentos,\n"
"        % if user.signature:\n"
"           <br/>\n"
"            ${user.signature | safe}\n"
"        % endif\n"
"    </p>\n"
"</div>\n"
"        "

#. module: partner_statement_send_by_mail
#: model:mail.template,body_html:partner_statement_send_by_mail.email_template_partner_outstanding_statement
msgid ""
